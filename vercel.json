{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/docs",
            "dest": "backend/main.py"
        },
        {
            "src": "/redoc",
            "dest": "backend/main.py"
        },
        {
            "src": "/health",
            "dest": "backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ]
}